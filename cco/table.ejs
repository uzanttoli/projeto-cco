<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Page Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
  </head>
  <body>
    <table>
      <thead>
        <% header.forEach(col => {%>
        <th><%= col %></th>
        <%})%>
      </thead>
      <tbody>
        <% rows.forEach(row => {%>
        <tr>
          <% row.forEach(element => {%>
          <td><%= element %></td>
          <% })%>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
